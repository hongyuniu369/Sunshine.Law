ul.nav1 li{float:left;  margin-bottom:10px; margin-left:9px; width:auto;  white-space:nowrap; word-spacing:normal;}
ul.nav2{width:170px; float:left; background:url(nav.png) no-repeat 14px right; display:block; height:46px; padding-top:15px; padding-left:15px; margin-top:10px; padding-right:5px; display:inline}
ul.nav2 li{float:left;  margin-bottom:10px; margin-left:9px; width:auto;  white-space:nowrap; word-spacing:normal;}

.navx{width:980px; font-size:14px; height:75px; margin:0 auto}
