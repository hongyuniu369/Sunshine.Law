#logo2 {float:left; width:58px;height:53px;}
#menu {float:left;width:770px;height:85px;padding-top:8px;padding-left:12px}
#menu ul{margin-bottom:8px;height:16px}
#menu ul li{float:left;color:black;margin-right:11px;font-size:14px}
#menu ul li a:link{color:black;}
#menu ul li a:hover{color:#d71f1d;}
#menu ul li a:visited{color:#000;}
#difang{padding:10px;height:80px;width:980px;overflow:hidden}
#difang span{margin:5px 10px;height:10px;line-height:20px;white-space:nowrap;font-size:12px;overflow:hidden}
#difang span a{color:#152288;}
#shengyu{padding:10px;height:80px;width:980px}
#shengyu span{margin:5px 10px;height:10px;text-decoration:none;line-height:20px;white-space:nowrap}
