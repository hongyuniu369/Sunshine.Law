@charset "utf-8";
/* CSS Document */
body,div,h1,h2,h3,h4,p,table,tr,td,ul,ol,li,dl,dd,dt,img,form,input{margin:0;padding:0;}
body{background:#fff;font-size:12px; font-family:"宋体",Arial,Verdana;color:#000; margin:0px auto;}
div,p,li{font-family:"宋体",Arial,Verdana; }
img{border: 0px;}
ul,li{ list-style:none;text-align:left;}
.fl{float:left;}
.fr{float:right;}
.cl{clear:both;}
/*list通用样式*/
.f12h22 ul li{font-size:12px;height:22px;line-height:22px;}
.f14h24 ul li{font-size:14px;height:24px;line-height:24px;}
.f14h22 ul li{font-size:14px;height:22px;line-height:22px;}
.f12h18 ul li{font-size:12px;height:18px;line-height:18px;}
.f12h21 ul li{font-size:12px;height:21px;line-height:21px;}
.f12h24 ul li{font-size:12px;height:24px;line-height:24px;}
.b0{background:none!important}
.black{color:black!important}
.black a:link{color:black!important}
.white{color:white!important}
.white a:link{color:white!important}
.w980{width:980px;}
#login{width:1000px;height:30px;line-height:30px;border-bottom:1px #ececec solid;background: white url('bg_login.gif') repeat-x}
.login0623 {float:left; width:380px; height:30px; overflow:hidden;}
.name{width:auto;float:left;padding-left:5px;color:black;}
.buttom{width:auto;float:left;margin-top:5px;margin-right:5px;margin-left:2px;}
.password{width:auto;float:left;line-height:30px;color:black;}
.mail_se{width:auto;float:left;line-height:30px;margin-top:5px}
.lo{width:40px;height:19px;float:left;background:white url('se1_bg.jpg') no-repeat 0 50%;margin:5px;text-align:center;line-height:19px;color:black;}
.re{width:auto;float:left;line-height:30px;}
.tianqi{width:auto;float:left;line-height:30px;color:red}
.buttom2{width:100px;float:left;margin-top:0px;margin-right:5px;margin-left:5px;color:#aaaaaa}
