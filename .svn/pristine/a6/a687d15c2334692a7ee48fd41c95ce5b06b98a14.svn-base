.zhuban{width:auto;float:left;line-height:30px;margin-left:5px;color:red;display:inline;}
.zhuban span{display:block;float:left;margin-left:5px}
.zhuban a:link{color:red}
.zhuban a:visited{color:red}
#head{ clear:both;height:53px;margin:5px auto;border-bottom:2px #165aa5 solid;background:white}
#logo {float:left; width:185px;height:53px;}
