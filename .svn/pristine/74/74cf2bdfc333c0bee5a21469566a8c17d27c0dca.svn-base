
<script>
    $(document).ready(function () {
        var rmzxViewModel = {
            items: ko.observableArray()
        };
        $.ajax({
            type: 'GET',
            url: '/api/zx/getrmzx/',
            async: false,
            success: function (d) {
                rmzxViewModel.items(ko.mapping.fromJS(d)());
            }
        });
        ko.applyBindings(rmzxViewModel, document.getElementById('rmzx'));
    });
</script>
